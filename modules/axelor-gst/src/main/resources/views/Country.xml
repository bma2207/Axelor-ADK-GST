<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">


	<grid name="country-grid" model="com.axelor.gst.db.Country"  title="country-grid">
		<field name="name"  />
	</grid>

	<form name="country-form" model="com.axelor.gst.db.Country" title="country-form" width="large" css="panel-pink">
		<panel title="Country" colSpan="12">
			<field name="name" required="true" />
		</panel>
		<panel sidebar="true">
			<field name="createdOn" />
			<field name="updatedOn" />
		</panel>
	</form>
</object-views>
