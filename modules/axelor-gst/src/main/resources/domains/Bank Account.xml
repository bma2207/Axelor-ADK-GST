<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module package="com.axelor.gst.db" name="gst" />
	<entity name="Bank">
		<string name="bank" title="Bank Name"/>
		<string name="accountNo" title="Account No"/>
		<string name="IFSC" title="IFSC Code"/>
		<string name="type" selection="gst-account-type-select" title="Account Type"/>
		<string name="address" large="true" title="Branch"/>
		
	</entity>
</domain-models>
