<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module package="com.axelor.gst.db" name="gst" />
	<entity name="Address">
		<string name="type" selection="gst-address-type-select" required="true"/>
		<string name="line1" required="true" namecolumn="true"/>
		<string name="line2"/>
		<many-to-one ref="Country" name="country"/>
		<many-to-one ref="State" name="state" />
		<many-to-one ref="City" name="city" />
		<string name="pinCode"/>
		
		
	</entity>
</domain-models>
